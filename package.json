{
  "name": "@jymfony/dependency-injection",
  "version": "0.1.0-dev",
  "description": "Jymfony Dependency Injection",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "node ./tests.js"
  },
  "keywords": [
    "di",
    "dependency injection",
    "jymfony"
  ],
  "author": "Alessandro Chitolina <alekitto@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@jymfony/autoloader": "https://github.com/jymfony/autoloader.git",
    "@jymfony/config": "https://github.com/jymfony/config.git",
    "@jymfony/datastructure": "https://github.com/jymfony/data-structure.git",
    "@jymfony/decorators": "https://github.com/jymfony/decorators.git",
    "@jymfony/exceptions": "https://github.com/jymfony/exceptions.git",
    "@jymfony/property-access": "https://github.com/jymfony/property-access.git",
    "@jymfony/util": "https://github.com/jymfony/util.git"
  },
  "devDependencies": {
    "@jymfony/debug": "https://github.com/jymfony/debug.git",
    "@jymfony/testing": "https://github.com/jymfony/testing.git",
    "@jymfony/var-dumper": "https://github.com/jymfony/var-dumper.git",
    "@jymfony/yaml": "https://github.com/jymfony/yaml.git",
    "chai": "^4.2",
    "mocha": "^6.0"
  },
  "config": {
    "jymfony-autoload": {
      "namespaces": {
        "Jymfony.Component.DependencyInjection": "src/"
      }
    },
    "jymfony-autoload-dev": {
      "namespaces": {
        "Jymfony.Component.DependencyInjection.Fixtures": "fixtures/",
        "Jymfony.Component.DependencyInjection.Tests": "test/"
      }
    }
  }
}
